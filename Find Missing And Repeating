#User function Template for python3

class Solution:
    def findTwoElement( self,arr, n): 
        # code here
        arr.sort()
        a=1
        for ele in set(arr):
            if a<ele:
                break
            else:
                a+=1
        i=0
        while i<n-1:
            if arr[i]==arr[i+1]:
                b= arr[i]
            i+=1
        return [b,a]
