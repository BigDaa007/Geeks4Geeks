class Solution:
    # Complete this function
    
    #Function to find equilibrium point in the array.
    def equilibriumPoint(self,A, N):
        # Your code here
        if N==1:
            return 1
        elif N==2:
            return -1
        Lsum = 0
        Rsum = 0
        Summ=[]
        curr = 0
        for ele in A:
            curr = curr+ele
            Summ.append(curr)
            
        for i in range(1,N-1):
            Lsum = Summ[i]-A[i]
            Rsum = Summ[-1]-Summ[i]
            
            if Lsum==Rsum:
                return i+1
            else:
                continue
        
        return -1
